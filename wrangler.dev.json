{
  "compatibility_date": "2025-04-01",
  "main": "src/server/index.ts",
  "name": "nativor-chat-worker",
  "migrations": [
    {
      "new_sqlite_classes": ["Chat"],
      "tag": "v1"
    }
  ],
  "build": {
    "command": "echo 'No build required'"
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "Chat",
        "name": "Chat"
      }
    ]
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "dev": {
    "port": 8787
  },
  "vars": {
    "BACKEND_ORIGIN": "http://localhost:1337",
    "WEBHOOK_TOKEN": "your-secret-token"
  }
}
